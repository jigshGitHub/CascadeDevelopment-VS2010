@{
    ViewBag.Title = "MediaRequestReadyDownload";
}

@Scripts.Render("~/bundles/jquery")
<h2>
    MediaRequest - Accounts with Requested Media Ready for Download</h2>
<table id="downloadableMedias" class="display">
</table>
@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#downloadableMedias").load(baseUrl + '/Recourse/Media/GetMediaRequestReadyDownload/');
        });
    </script>
}
