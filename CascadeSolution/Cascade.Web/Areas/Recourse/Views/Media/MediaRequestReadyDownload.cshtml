@{
    ViewBag.Title = "MediaRequestReadyDownload";
}

@Scripts.Render("~/bundles/jquery")
<h2>
    MediaRequest - Accounts with Requested Media Ready for Download</h2>
<table id="downloadableMedias" class="display">
</table>
@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            var href = window.location.href.split('/');
            var baseUrl = href[0] + '//' + href[2] + '/' + href[3];
            $("#downloadableMedias").load(baseUrl + '/Media/GetMediaRequestReadyDownload/');
        });
    </script>
}
